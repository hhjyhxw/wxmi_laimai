.content{
	height: 583px;
	margin-bottom: 5rem;
}
.content .wrapper-nav {
      position: static;
      float: left;
      width: 28.125%;
      height: 100%;
      background-color: #f8f8f8;
      overflow: auto;
      -webkit-overflow-scrolling: touch;
}
.content .wrapper-nav .scroller {
        height: 100%;
        padding-bottom: 4rem;
        box-sizing: border-box;
}
