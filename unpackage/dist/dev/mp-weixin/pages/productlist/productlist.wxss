.addToCaret {
    display: block;
    color: transparent;
    position: absolute;
    height: 24px;
    width: 24px;
    right: 1.5rem;
    bottom: 1.5rem;
    background: url(data:image/png;base64,) 100% 100% no-repeat scroll;
    background-size: 24px 24px;
}
.addToCaret.disabled {
     /* background: url(../../static/image/btn_gg_tj_disabled.png) 100% 100% no-repeat scroll; */
      background-size: 24px 24px;
}
.link .wrap.over .goodbefore:before {
	      content: '';
	      display: block;
	      position: absolute;
	      top: 0;
	      left: 0;
	      right: 0;
	      bottom: 0;
	      background: url(data:image/png;base64,) 50% 35% no-repeat rgba(0, 0, 0, 0.3);
	      background-size: 4.75rem auto;
		      z-index: 2;
}
