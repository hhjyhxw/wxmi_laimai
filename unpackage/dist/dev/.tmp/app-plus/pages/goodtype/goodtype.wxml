<view><view class="all_shop"><block wx:for="{{supplierList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="link-shop"><view><view data-event-opts="{{[['tap',[['toShopDetail',['$0'],[[['supplierList','',index,'id']]]]]]]}}" class="shop-name shop-name-a" bindtap="__e"><image class="shop-icon" src="{{item.supplierImg}}"></image><view class="shop_detail_name">{{item.companyName}}</view><view class="time_send"><text class="time">{{item.businessHours}}</text><text class="send">免费配送</text></view></view></view><view class="shop-goods"><block wx:for="{{item.goodsList}}" wx:for-item="item2" wx:for-index="index2" wx:key="index2"><view class="wrap norest"><view data-event-opts="{{[['tap',[['toGoodsDetail',['$0'],[[['supplierList','',index],['goodsList','',index2,'id']]]]]]]}}" class="good" bindtap="__e"><view class="{{['good_img',(item2.maiwan)?'good_img_icon':'']}}"><image src="{{item2.defaultSourceImagePath}}"></image></view><view class="txt-normal">{{item2.name}}</view><view class="price">{{"￥"+item2.marketPrice}}</view></view><view data-event-opts="{{[['tap',[['addCart',['$0'],[[['supplierList','',index],['goodsList','',index2,'pid']]]]]]]}}" class="addToCaret" bindtap="__e">添加</view></view></block></view></view></block></view><view data-event-opts="{{[['tap',[['toCart',['$event']]]]]}}" class="settle show" bindtap="__e"><view class="left"><view class="left_one"><view class="caret-icon"><label>{{cartNum}}</label></view><view class="caret_num"><label>{{cartNum}}</label></view></view><view class="total-money"></view></view><view class="right">去结算</view></view></view>