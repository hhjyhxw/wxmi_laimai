<view class="page"><view class="header1"><view data-event-opts="{{[['tap',[['getLocationInfo',['$event']]]]]}}" class="lbs-btn" bindtap="__e"><text>{{address}}</text></view></view><view class="search-box"><m-search class="mSearch-input-box" vue-id="1" mode="{{2}}" button="inside" placeholder="{{defaultKeyword}}" value="{{keyword}}" data-event-opts="{{[['^touchstart',[['sboxblur']]],['^search',[['doSearch',[false]]]],['^input',[['__set_model',['','keyword','$event',[]]],['inputChange']]],['^confirm',[['doSearch',[false]]]]]}}" bind:touchstart="__e" bind:search="__e" bind:input="__e" bind:confirm="__e" bind:__l="__l"></m-search></view><view hidden="{{!(goodslistbox)}}" class="allshop"><block wx:for="{{supplierList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toShopDetail',['$0'],[[['supplierList','',index,'id']]]]]]]}}" class="shoplist" bindtap="__e"><view class="shop_img"><image class="shop_img_in" src="{{item.supplierImg}}"></image></view><view class="shop_info"><view class="shop_name">{{item.storeName}}</view><view class="shop_busnesstime">{{"服务时间："+item.businessHours}}</view><view class="shop_address">{{"地址:"+item.address}}</view><view class="shop_tel">{{"电话:"+item.contactPhone}}</view><view class="shop_distance">{{item.disatance+"m"}}</view></view><view class="shop_go"></view></view></block></view><back-top vue-id="2" src="{{backTop.src}}" scrollTop="{{backTop.scrollTop}}" bind:__l="__l"></back-top></view>