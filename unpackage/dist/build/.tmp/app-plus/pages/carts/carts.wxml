<view class="page"><block wx:if="{{supplierList!=null&&supplierList.length>0}}"><view class="main-container"><view class="header"><view class="gwc"></view><view data-event-opts="{{[['tap',[['deleteAll',['$event']]]]]}}" class="del-caret" bindtap="__e"></view></view><block wx:for="{{supplierList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="caret-list"><view class="shop_header"><view class="selectshoplabl"><radio class="selectShop" checked="{{item.checked}}" data-event-opts="{{[['tap',[['selectShop',[index]]]]]}}" bindtap="__e"></radio></view><label class="shop_name">{{item.supplierName}}</label><label data-event-opts="{{[['tap',[['toShopDetail',['$0'],[[['supplierList','',index,'id']]]]]]]}}" class="arr" bindtap="__e"></label></view><block wx:for="{{item.careItemList}}" wx:for-item="item2" wx:for-index="index2" wx:key="index2"><view class="goodsList"><view class="col1"><radio class="selectOne" checked="{{item2.checked}}" data-event-opts="{{[['tap',[['selectOne',[index,index2]]]]]}}" bindtap="__e"></radio></view><view class="col2"><image class="good_img" src="{{item2.listImgPath}}"></image></view><view class="col3"><view class="goodName">{{item2.pName}}</view><view class="calc"><label data-event-opts="{{[['tap',[['minus',[index,index2]]]]]}}" class="minus" bindtap="__e">减一</label><input class="goodsNum" type="number" data-event-opts="{{[['input',[['updateGoodNum',[index,index2,'$event']]]]]}}" value="{{item2.quantity}}" bindinput="__e"/><label data-event-opts="{{[['tap',[['plus',[index,index2]]]]]}}" class="plus" bindtap="__e">加一</label></view></view><view class="col4"><view class="p1"></view><view class="p2">{{"￥"+item2.price}}</view><view data-event-opts="{{[['tap',[['deleteOne',[index,index2]]]]]}}" class="del-list" bindtap="__e">删除</view></view></view></block></view></block></view></block><block wx:if="{{supplierList!=null&&supplierList.length>0}}"><view class="footer-count"><view class="total"><view class="flex"><view class="sp1"><radio class="selectAll" checked="{{selectAllChecked}}" data-event-opts="{{[['tap',[['selectAll',['$event']]]]]}}" bindtap="__e">全选</radio></view><view class="sp2">合计:￥<label>{{totalMoney}}</label></view></view></view><view data-event-opts="{{[['tap',[['toPrepay',['$event']]]]]}}" class="submit" bindtap="__e">去结算</view></view></block><block wx:if="{{(supplierList==null||supplierList==''||supplierList.length==0)&&accessToken!=null}}"><view class="content"><view class="uni-cart-imgbox"><image class="uni-cart-img" src="../../static/image/pic_gw_k.png"></image></view><view class="uni-cart-text1">您的购物车空空哒~</view><view data-event-opts="{{[['tap',[['goToIndex',['$event']]]]]}}" class="uni-cart-btn" bindtap="__e"><text class="uni-cart-text1">去逛逛</text></view></view></block><block wx:if="{{accessToken==null}}"><view class="content"><view data-event-opts="{{[['tap',[['tologin',['$event']]]]]}}" class="login_btn" bindtap="__e"><view class="login_text">登陆/注册</view></view></view></block></view>