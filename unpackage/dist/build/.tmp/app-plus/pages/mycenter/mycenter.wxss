.carousel {
  box-sizing: border-box;
}
.userInfo {
  width: 100%;
  padding: 1rem;
  box-sizing: border-box;
  /*  background-color: lightblue; */
  background-color: white;
  border-bottom: 1px solid #EBEBEB;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.heardurl {
  width: 2.5rem;
  height: 2.5rem;
  display: block;
  margin-right: 0.5rem;
  border: 1px solid #CDCDCD;
  border-radius: 100%;
}
.userInfo .user {
  display: block;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  color: black;
  line-height: 1.6;
  background: url(data:image/png;base64,) 100% 50% no-repeat;
  background-size: 5.5px auto;
}
.carousel .userInfo .user .name {
  font-size: 0.8rem;
}
.userInfo .user .phone {
  font-size: 0.7rem;
}
.userInfo .user .phone text {
  width: 1.2rem;
  height: 2rem;
  color: black;
  background: url(data:image/png;base64,) left no-repeat;
  background-position: -0.5rem 0;
  background-size: 3.1rem;
  display: inline-block;
  vertical-align: middle;
}
.list-group {
  padding: 0 0 0 1.2rem;
  margin: 0.5rem 0;
  border-top: 1px solid #EBEBEB;
  border-bottom: 1px solid #EBEBEB;
  background-color: #fff;
}
.list-group .list-group-item {
  position: relative;
  display: block;
  padding: 0 1.5rem 0 0;
  border-bottom: 1px solid #EBEBEB;
  height: 2.3rem;
  line-height: 2.3rem;
  border-bottom: 1px solid #EBEBEB;
  font-size: 0.7rem;
}
.list-group .list-group-item .icon {
  width: 2rem;
  background-size: 18px 18px;
  vertical-align: middle;
  display: inline-block;
  height: 2rem;
  vertical-align: middle;
  padding-right: .4rem;
}
.list-group .list-group-item .arr {
  background: url(data:image/png;base64,) 100% 50% no-repeat;
  background-size: 5.5px auto;
  height: 2rem;
  width: 2rem;
  display: block;
  position: absolute;
  right: 1.6rem;
  top: 0;
}
.list-group-item .badge2 {
  display: block;
  position: absolute;
  right: 3.6rem;
  top: 0.4rem;
  background-color: #E13F3F;
  height: 1.2rem;
  line-height: 1.2rem;
  padding: 0 .6em;
  font-size: 1rem;
  color: #fff;
  text-align: center;
  white-space: nowrap;
  vertical-align: baseline;
  border-radius: 20%;
}
.list-group .list-group-item .icon1 {
  background: url(data:image/png;base64,) 0 50% no-repeat;
  background-size: 2rem 2rem;
}
.list-group .list-group-item .icon2 {
  background: url(data:image/png;base64,) 0 50% no-repeat;
